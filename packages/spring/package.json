    "chalk": "^5.3.0",
    "docx-pdf": "^0.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "grammy": "^1.28.0",
    "html-pdf": "^3.0.1",
    "mammoth": "^1.9.0",
    "marked": "^13.0.3",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "openai": "^4.53.2",
    "types-json": "^4.0.1",
    "zod": "^3.24.2"
  },
