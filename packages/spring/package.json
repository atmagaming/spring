{
  "name": "@spring/spring",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run ./dist/index.js",
    "dev": "bun run ./src/index.ts",
    "dev:local": "env-cmd -f .env.local-override bun run --watch ./src/index.ts ",
